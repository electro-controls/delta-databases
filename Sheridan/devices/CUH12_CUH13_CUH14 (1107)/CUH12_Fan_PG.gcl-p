//fan program 

'CUH12_Enable' = Switch ( 'CUH12_Enable', 'CUH12_EffZoneHtg', 90, 10)
 

//fan status
'CUH12_FanStatus' = Switch ( 'CUH12_FanStatus', 'CUH12_Current', 0.7, 0.4)

