//fan program 

'CUH3_Enable' = Switch ( 'CUH3_Enable', 'CUH3_EffZoneHtg', 90, 10)
 

//fan status
'CUH3_FanStatus' = Switch ( 'CUH3_FanStatus', 'CUH3_Current', 0.7, 0.4)

