//fan program 

'CUH6_Enable' = Switch ( 'CUH6_Enable', 'CUH6_EffZoneHtg', 90, 10)
 

//fan status
'CUH6_FanStatus' = Switch ( 'CUH6_FanStatus', 'CUH6_Current', 0.7, 0.4)

