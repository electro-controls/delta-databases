//fan program 

'CUH8_Enable' = Switch ( 'CUH8_Enable', 'CUH8_EffZoneHtg', 90, 10)
 

//fan status
'CUH8_FanStatus' = Switch ( 'CUH8_FanStatus', 'CUH8_Current', 0.7, 0.4)

