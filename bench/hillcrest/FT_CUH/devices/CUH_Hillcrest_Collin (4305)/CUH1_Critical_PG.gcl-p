'CUH1_Critical_PG.Write_Priority' = 4

//software freeze mode

If ('CUH1_Eff_ZoneTemp' < 42) OnFor 60S Then
    'CUH1_FreezeMode' = On
End If

If ('CUH1_Eff_ZoneTemp' > 45) OnFor 60S Then
    'CUH1_FreezeMode' = Off
End If

If PowerUp Then
    'CUH1_FreezeMode' = Off
End If


If 'CUH1_FreezeMode' Then
    'HWV' = On
    'FanEnable' = Off
  Else
    Relinquish 'HWV'
    Relinquish 'FanEnable'
End If


//end of critical page