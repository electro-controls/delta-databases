'Critical_PG.Write_Priority' = 4

If 'Hi_Temp_Limit' Or ('Pool_Supply_Temp' > 'PoolHWS_HiLimit_Sp') OnFor 30S Then
    'HighPoolLimit' = On
End If
If (Not 'Hi_Temp_Limit') And ('Pool_Supply_Temp' < 'PoolHWS_EffSp') Then
    'HighPoolLimit' = Off // todo send alarm
End If

If 'HighPoolLimit' Then
  'HW_FeedValve' = 0
  'HX_Pump_Enable' = Off
 Else
  Relinquish 'HX_Pump_Enable'
  Relinquish 'HW_FeedValve'
End If