'Critical_PG.Write_Priority' = 4

If 'Pool_Supply_Temp' > 'PoolHWSMax_Sp' Then
    'HighPoolLimit' = On
End If
If ('HighPoolLimit' OnFor 5M) And ('Pool_Supply_Temp' < 'PoolHWS_EffSp') Then
    'HighPoolLimit' = Off // todo send alarm
End If

If 'HighPoolLimit' Or 'Hi_Temp_Limit' Then
  'HW_FeedValve' = 0
  'HX_Pump_Enable' = Off
 Else
  Relinquish 'HX_Pump_Enable'
  Relinquish 'HW_FeedValve'
End If