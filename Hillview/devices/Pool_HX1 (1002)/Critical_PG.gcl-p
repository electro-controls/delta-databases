'Critical_PG.Write_Priority' = 4

If 'Pump_Current' > ('PoolHWR_Sp' + 8) Then
    'HighPoolLimit' = On
End If
If ('HighPoolLimit' OnFor 5M) And ('Pump_Current' < 'PoolHWR_Sp') Then
    'HighPoolLimit' = Off
End If

If (Not 'PumpStatus') Or 'HighPoolLimit' Then
  'HW_FeedValve' = 0
 Else
  Relinquish 'HW_FeedValve'
End If